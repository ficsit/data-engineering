  "description": "FicsIt's official data engineering toolbox.",
  "repository": "https://github.com/ficsit/data-engineering",
  "author": "FicsIt Employees",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "pipeline/*"
  ],
  "scripts": {
