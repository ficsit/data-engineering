  },
  "dependencies": {},
  "devDependencies": {
    "@ficsit/code-style": "^1.4.2",
    "@types/node": "^13.9.0",
    "eslint": "^6.8.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
